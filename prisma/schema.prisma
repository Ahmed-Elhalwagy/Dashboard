generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "mysql"
  url       = env("DATABASE_URL")
  directUrl = env("MAIN_DATABASE_URL")
}

model ArchivedJob {
  id       String @id @unique
  company  String
  title    String
  link     String
  email    String
  type     String
  deadline String
  logo     String
  skills   String
}

model CrawledJob {
  id       String @id @unique
  company  String
  title    String
  link     String
  email    String
  type     String
  deadline String
  logo     String
  skills   String
}

model Job {
  id       String @id @unique
  company  String
  title    String
  link     String
  email    String
  type     String
  deadline String
  logo     String
  skills   String

  @@index([title])
}

model Lister {
  id    String @id @unique
  name  String
  token String
}
